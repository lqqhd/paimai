<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xiaoxing.login">

    <application>

        <!-- 在组件集成宿主时, 由于宿主已经声明了 CommonSDK 的公有 ConfigModule, 所以在这里只需要声明业务组件自己的 ConfigModule -->
        <meta-data
            android:name="com.xiaoxing.login.app.GlobalConfiguration"
            android:value="ConfigModule" />

        <activity android:name=".mvp.ui.activity.LoginActivity" />
        <activity android:name=".mvp.ui.activity.ProtocolActivity" />
        <activity android:name=".mvp.ui.activity.RegisterSendPhoneActivity" />
        <activity android:name=".mvp.ui.activity.RegisterVerificationCodeActivity" />
        <activity android:name=".mvp.ui.activity.ForgotPwdSendPhoneActivity" />
        <activity android:name=".mvp.ui.activity.ForgotPwdVerificationCodeActivity" />
        <activity android:name=".mvp.ui.activity.SetNewPasswordActivity"></activity>
    </application>

</manifest>