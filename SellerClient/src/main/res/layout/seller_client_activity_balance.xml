<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F5F5F5"
    android:orientation="vertical">


    <include layout="@layout/public_toolbar" />

    <RelativeLayout
        android:id="@+id/rl_wo_de_zi_chan"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_50"
        android:background="@drawable/public_ripple_bg">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dp_10"
            android:drawableLeft="@mipmap/mem_icon9"
            android:drawablePadding="@dimen/dp_10"
            android:gravity="center_vertical"
            android:text="我的资产"
            android:textColor="@color/seller_client_mine_order"
            android:textSize="@dimen/public_font_16sp" />

        <ImageView
            android:id="@+id/imageView14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginBottom="@dimen/dp_168"
            android:layout_marginEnd="@dimen/dp_15"
            android:layout_marginRight="@dimen/dp_15"
            android:src="@drawable/icon_next" />

        <TextView
            android:id="@+id/tv_yu_e_ming_xi"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginEnd="@dimen/dp_10"
            android:layout_marginRight="@dimen/dp_10"
            android:layout_toLeftOf="@+id/imageView14"
            android:layout_toStartOf="@+id/imageView14"
            android:text="余额明细"
            android:textColor="@color/seller_client_mine_see_all_orders"
            android:textSize="@dimen/public_font_12sp" />

    </RelativeLayout>

    <include layout="@layout/seller_client_include_available_balance" />


    <RelativeLayout
        android:id="@+id/rl_chong_zhi"
        style="@style/public_MineItemRelativeLayout"
        android:layout_marginTop="@dimen/dp_10">


        <TextView
            style="@style/public_MineItemText"
            android:text="充值" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/dp_15"
            android:drawablePadding="@dimen/dp_10"
            android:drawableRight="@drawable/icon_next"
            android:text=""
            android:textColor="@color/public_mine_item_text"
            android:textSize="@dimen/public_font_14sp" />

    </RelativeLayout>

    <include layout="@layout/public_line_horizontal" />

    <RelativeLayout
        android:id="@+id/rl_ti_xian"
        style="@style/public_MineItemRelativeLayout">


        <TextView
            style="@style/public_MineItemText"
            android:text="提现" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/dp_15"
            android:drawablePadding="@dimen/dp_10"
            android:drawableRight="@drawable/icon_next"
            android:text=""
            android:textColor="@color/public_mine_item_text"
            android:textSize="@dimen/public_font_14sp" />

    </RelativeLayout>

    <include layout="@layout/public_line_horizontal" />

    <RelativeLayout

        android:id="@+id/rl_xiao_bao_jin"
        style="@style/public_MineItemRelativeLayout">


        <TextView
            style="@style/public_MineItemText"
            android:text="我的消保金" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/dp_15"
            android:drawablePadding="@dimen/dp_10"
            android:drawableRight="@drawable/icon_next"
            android:text=""
            android:textColor="@color/public_mine_item_text"
            android:textSize="@dimen/public_font_14sp" />

    </RelativeLayout>

    <include layout="@layout/public_line_horizontal" />

    <RelativeLayout
        android:id="@+id/tl_yin_hang_ka"
        style="@style/public_MineItemRelativeLayout"
        android:layout_marginTop="@dimen/dp_10">


        <TextView
            style="@style/public_MineItemText"
            android:text="管理银行卡" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/dp_15"
            android:drawablePadding="@dimen/dp_10"
            android:drawableRight="@drawable/icon_next"
            android:text=""
            android:textColor="@color/public_mine_item_text"
            android:textSize="@dimen/public_font_14sp" />

    </RelativeLayout>

    <include layout="@layout/public_line_horizontal" />

    <RelativeLayout
        android:id="@+id/rl_zhi_fu_an_quan"
        style="@style/public_MineItemRelativeLayout">


        <TextView
            style="@style/public_MineItemText"
            android:text="支付安全" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/dp_15"
            android:drawablePadding="@dimen/dp_10"
            android:drawableRight="@drawable/icon_next"
            android:text=""
            android:textColor="@color/public_mine_item_text"
            android:textSize="@dimen/public_font_14sp" />

    </RelativeLayout>
</LinearLayout>