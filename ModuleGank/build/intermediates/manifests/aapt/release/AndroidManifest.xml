<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="me.jessyan.armscomponent.gank"
    android:versionCode="1"
    android:versionName="1.0.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="27" />

    <application>
        <activity
            android:name="me.jessyan.armscomponent.gank.mvp.ui.activity.GankHomeActivity"
            android:configChanges="orientation|screenSize"
            android:label="@string/public_name_gank"
            android:screenOrientation="portrait" />
        <!-- 在组件集成宿主时, 由于宿主已经声明了 CommonSDK 的公有 ConfigModule, 所以在这里只需要声明业务组件自己的 ConfigModule -->
        <meta-data
            android:name="me.jessyan.armscomponent.gank.app.GlobalConfiguration"
            android:value="ConfigModule" />
    </application>

</manifest>